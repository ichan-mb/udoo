<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="login_layout" inherit_id="web.login_layout">
        <xpath expr="//a[@href='https://www.odoo.com?utm_source=db&amp;utm_medium=auth']" position="replace">
            <a t-att-href="brand_url" target="_blank">
                <span>
                    Powered by <t t-out="brand_name"/>
                </span>
            </a>
        </xpath>
    </template>
    <template id="udoo_layout" inherit_id="web.layout">
        <xpath expr="//link[1]" position="after">
        <link href="https://cdn.jsdelivr.net/npm/remixicon@4.2.0/fonts/remixicon.css" rel="stylesheet" />
        </xpath>
    </template>
</odoo>
